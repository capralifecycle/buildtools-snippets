{
  "extends": ["github>capraconsulting/renovate-config:default"],
  "pinDigests": true,
  "digest": {
    "automerge": true,
    "automergeType": "branch"
  },
  "regexManagers": [
    {
      "fileMatch": ["/install\\.sh$"],
      "matchStrings": [
        "datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?( extractVersion=(?<extractVersion>.*?))?( versioningTemplate=(?<versioningTemplate>.*?))?\\s.*?_VERSION=(?<currentValue>.*)\\s"
      ]
    }
  ]
}
